package Pertemuan2;

public class Main {
    
    public static void main(String[] args) {
        String nama = "Futri";
        System.out.println("nama saya \t\t: "+nama);
        
        String alamat = "Tapalang";
        System.out.println("saya berdomisili di \t:"+alamat);
        
        String handphone = "085325792461";
        System.out.println("info kontak \t\t: "+handphone);
        
        char kelas = 'A';
        System.out.println(" dari kelas informatika\t: "+kelas);
    }
}
